filters:
  and:
    - file.inFolder("NoteAboutStudy/book/CSAPP")
    - file.name != "content.base"
    - file.folder != "NoteAboutStudy/book/CSAPP/labs"
views:
  - type: table
    name: 表格
    filters:
      and:
        - '!file.inFolder("study/book/CSAPP/labs")'
    order:
      - file.name
      - file.ctime
    sort:
      - property: file.name
        direction: ASC
